record line-segment {
    start: point,
    end: point
}

record circle {
    center: point,
    radius: float32
}

record multi-line{
    points: list<point>,
}

perimeter-of-circle: func(c: circle) -> float32

area-of-circle: func(c: circle) -> float32

multi-line-length: func(l: multi-line) -> float32
